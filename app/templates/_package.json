{
  "name": "<%= moduleName %>",
  "version": "0.0.1",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= repoName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "xo && ava",
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup"
  },
  "files": [
    "index.js",
    "icon.png",
    "info.plist"
  ],
  "keywords": [
    "alfred",
    "workflow",
    "alfy"
  ],
  "dependencies": {
    "alfy": "<%= alfyVersion %>"
  },
  "devDependencies": {
    "alfy-test": "<%= alfyTestVersion %>",
    "ava": "<%= avaVersion %>",
    "xo": "<%= xoVersion %>"
  }
}
